<header>
    <nav class="navbar navbar-expand-lg">
        <div class="container-fluid">
            <div class="navbar-brand"><%= link_to "福祉.net", root_path %></div>
            <ul class="navbar navbar-right">
                <li class="navbar-text"><%= link_to "users", users_path %></li>
                <li class="navbar-text"><%= link_to "post", "#" %></li>
                <li class="navbar-text"><%= link_to "about", about_path %></li>
            <% if logged_in? %>
                <li class="navbar-text"><%= link_to "#{current_user.name}_page", user_path(current_user) %></li>
                <li class="navbar-text"><%= @current_user.name %>でログイン中</li>
                <li class="navbar-text"><%= link_to "logout", logout_path, method: :delete %></li>
            <% else %>
                <li class="navbar-text"><%= link_to "signup", signup_path %></li>
                <li class="navbar-text"><%= link_to "login", login_path %></li>
            <% end %>
            </ul>
        </div>
    </nav>
</header>