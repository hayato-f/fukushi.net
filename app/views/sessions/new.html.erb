<h1>< ログイン ></h1>
<div class="form">
    <%= form_with(url: login_path, scope: :session, local: true) do |f| %>
        <div>
            <%= f.label :メールアドレス %>
            <%= f.email_field :email %>
        </div>
        <div>
            <%= f.label :パスワード %>
            <%= f.password_field :password %>
        </div>
        <div><%= f.submit "ログイン" %></div>
    <% end %>
</div>
